{
  "name": "@smart-clip/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "workers": "tsx src/workers/index.ts",
    "build": "tsc",
    "start": "node --import tsx/esm src/index.ts",
    "start:prod": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "check-jobs": "node check-jobs.js",
    "check-status": "node check-status.js",
    "clear-queue": "node clear-queue.js",
    "debug-queue": "node debug-queue.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.931.0",
    "@aws-sdk/lib-storage": "^3.931.0",
    "@aws-sdk/s3-request-presigner": "^3.931.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@google-cloud/speech": "^7.2.1",
    "@google-cloud/storage": "^7.17.3",
    "@google-cloud/video-intelligence": "^6.2.1",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.0",
    "@stripe/stripe-js": "^8.5.3",
    "@types/bull": "^4.10.4",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^11.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^5.9.0",
    "say": "^0.16.0",
    "stripe": "^20.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.19.25",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "eslint": "^9.15.0",
    "nodemon": "^3.1.11",
    "prisma": "^6.19.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}