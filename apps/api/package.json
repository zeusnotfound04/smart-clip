    "@google-cloud/video-intelligence": "^6.2.1",
    "@prisma/client": "^6.19.0",
    "@types/bull": "^4.10.4",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/multer": "^2.0.0",
